    "@react-navigation/native-stack": "^7.0.3",
    "@reduxjs/toolkit": "^2.2.7",
    "clsx": "^2.1.1",
    "expo": "^52.0.18",
    "expo-av": "^15.0.1",
    "expo-file-system": "^18.0.5",
    "expo-image": "^2.0.3",
    "expo-image-picker": "^16.0.3",
    "expo-localization": "^16.0.0",
